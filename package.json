{
  "name": "stats",
  "version": "3.0.0",
  "description": "Stats API for my about page",
  "main": "./graphql/index.ts",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "now dev",
    "deploy": "now",
    "lint-staged": "lint-staged",
    "typecheck": "tsc -p tsconfig.json --pretty --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lowmess/stats.git"
  },
  "author": "Alec Lomas <alec@lowmess.com>",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.7",
    "@types/node-fetch": "^2.5.5",
    "@types/xml2js": "^0.4.5",
    "abort-controller": "^3.0.0",
    "apollo-server-express": "^2.10.1",
    "apollo-server-plugin-response-cache": "^0.3.11",
    "aws-sdk": "^2.632.0",
    "date-fns": "^2.10.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "node-cache": "^5.1.0",
    "node-fetch": "^2.6.0",
    "typescript": "^3.8.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-lowmess": "^3.2.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "now": "^17.0.4",
    "prettier": "^1.19.1",
    "prettier-config-lowmess": "^0.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged && npm run typecheck"
